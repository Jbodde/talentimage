{
  "restApiRoot": "/api",
  "host": "process.env.VCAP_APP_HOST || 'localhost'",
  "port": "process.env.VCAP_APP_PORT || 443",
  "remoting": {
    "context": false,
    "rest": {
      "handleErrors": false,
      "normalizeHttpPath": false,
      "xml": false
    },
    "json": {
      "strict": false,
      "limit": "100kb"
    },
    "urlencoded": {
      "extended": true,
      "limit": "100kb"
    },
    "cors": false
  }
}
